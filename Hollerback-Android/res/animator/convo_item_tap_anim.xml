<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially" >

    <objectAnimator
        android:duration="70"
        android:interpolator="@android:anim/anticipate_interpolator"
        android:propertyName="rotation"
        android:valueFrom="0"
        android:valueTo="5" >
    </objectAnimator>
    <objectAnimator
        android:duration="140"
        android:propertyName="rotation"
        android:valueFrom="5"
        android:valueTo="-5" >
    </objectAnimator>
    <objectAnimator
        android:duration="70"
        android:propertyName="rotation"
        android:valueFrom="-5"
        android:valueTo="0" >
    </objectAnimator>

</set>