<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
    <declare-styleable name="CustomFont">
        <attr name="typeface" format="string" />
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="radius" format="dimension"></attr>
        <attr name="xPosCenter" format="dimension"></attr>
        <attr name="yPosCenter" format="dimension"></attr>
        <attr name="foregroundColor" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="CustomView">
        <attr name="borderColor" format="color"></attr>
        <attr name="borderWidth" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="CustomButton">
        <attr name="tintColor" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="CustomState">
        <attr name="emphasized" format="boolean"></attr>
    </declare-styleable>

</resources>